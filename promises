function fetchData(url) {
    return new Promise((resolve, reject) => {
        setTimeout(() => {
            if (url) {
                resolve(`Data fetched from ${url}`);
            } else {
                reject('Invalid URL');
            }
        }, 2000);
    });
}
fetchData()
   .then(data)=console.log(data)
   .catch(error => console.error(error));

