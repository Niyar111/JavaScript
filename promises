function getdata (irl){
    return new promises ((resolve, reject) =>{
        if (irl){
            resolve(`Data fetched from ${irl}`);
        }
        else {
            reject('Invalid URL');
        }
    }, 2000);
}

function addtion (a,b){
    return new promises ((resolve, reject ) =>{
        if (typeof a === 'number' && typeof b === 'number') {
            resolve(a + b);
        } else {
            reject('Both arguments must be numbers');
        }
    }, 2000);
}
